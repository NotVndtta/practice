<div id="<%= dom_id post %>">
  <p>
    <strong>Title:</strong>
    <%= post.title %>
  </p>

  <p>
    <strong>Description:</strong>
    <%= post.description %>
  </p>

  <p>
    <strong>Tags:</strong>
    <%= post.tags %>
  </p>
<% post.images.each do |image|%>
    <%= image_tag(image, style: "object-fit: contain; width: 450px; height: 450px;")%>
  <% end%>
</div>
